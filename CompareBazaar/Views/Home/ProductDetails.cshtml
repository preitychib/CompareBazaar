<section class=" container-fluid px-5 py-5">
    <div class="row my-5 ">
        <div class="col-xxl-4 col-xl-4 col-lg-4 col-md-4 col-sm-12 ">
            <div class=" border border-dark ">
                <!-- <span class="text-black  m-2  d-block" onclick="Wishlist(this)"> <i class="far fa-heart pointer   fontsize4 "></i></span> -->
                <div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="carousel">
                    <div class="carousel-indicators">
                        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active "
                                aria-current="true" aria-label="Slide 1"></button>
                        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1"
                                aria-label="Slide 2"></button>
                        <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2"
                                aria-label="Slide 3"></button>
                    </div>
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img src="@ViewBag.mobile.product_images.image" class="d-block w-100" alt="...">
                        </div>
                        <div class="carousel-item">
                            <img src="@ViewBag.mobile.product_images.image" class="d-block w-100" alt="...">
                        </div>
                        <div class="carousel-item">
                            <img src="@ViewBag.mobile.product_images.image" class="d-block w-100" alt="...">
                        </div>
                    </div>
                    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators"
                            data-bs-slide="prev">
                        <!-- <span class="carousel-control-prev-icon  " aria-hidden="true"></span> -->
                        <i class=" carousel-control  fas fa-chevron-left  text-black fs-2 "></i>
                        <span class="visually-hidden">Previous</span>
                    </button>
                    <button class="carousel-control-next  " type="button" data-bs-target="#carouselExampleIndicators"
                            data-bs-slide="next">
                        <!-- <span class="carousel-control-next-icon    " aria-hidden="true"></span> -->
                        <i class="carousel-control  fas fa-chevron-right text-black fs-2  "></i>
                        <span class="visually-hidden">Next</span>
                    </button>
                </div>

                <!-- <div class="border-top border-dark"> -->
                <div class="mt-3 mx-3 p-1 border-top border-dark d-flex justify-content-center  ">
                    <span class="text-black " onclick="Wishlist(this)"> <i class="far fa-heart pointer ms-4 mt-2  fontsize4 "></i> </span>
                    <a href="#" class="btn btn-danger redbtn bg-red my-2 mx-3 col-8 shadow-none">COMPARE</a>
                </div>
                <!-- </div> -->
            </div>
        </div>
        <div class=" col-xxl-8 col-xl-8 col-lg-8 col-md-8 col-sm-12 ">
            <div class="m-3">
                <h4 class="fw-bold fs-3 fontsize7">  @ViewBag.mobile.name</h4>
                <!-- <a href="" class="btn btn-success"> 4.2 <i class="fas fa-star"></i> </a> -->
            </div>
            <div class="m-4">
                <h5 class="fw-bold fs-3 text-decoration-underline fontsize7 ">Price Comparsion</h5>
                <div class="my-3 bg-light border border-dark ">
                    @if (ViewBag.mobile.vendor.name == "Amazon")
                    {
                        <div class="row p-2 ">
                            <div class="col-xxl-3 col-xl-3 col-lg-4 col-md-4 col-sm-6 col-xs-6 d-flex align-items-center  justify-content-center ">
                                <img class="logo" src="~/User/images/amazon-logo-removebg-preview.png" alt="Amazon logo">
                            </div>
                            <div class="col-xxl-3 col-xl-3 d-flex align-items-center hidename  justify-content-center  ">
                                <p class="text-black  ">  @ViewBag.mobile.name </p>
                            </div>
                            <div class="col-xxl-3 col-xl-3 col-lg-4 col-md-4 col-sm-6 col-xs-6 d-flex align-items-center  justify-content-center ">
                                <p class="text-red fontsize5 fw-bold"> @ViewBag.mobile.price</p>
                            </div>
                            <div class="col-xxl-3 col-xl-3 col-lg-4 col-md-4 col-sm-12 col-xs-12 d-flex align-items-center  justify-content-center block ">
                                <a class="btn btn-primary fontsize6 col-12 " href=" @ViewBag.mobile.url"> GO TO AMAZON <i class="fas fa-chevron-right"></i> </a>
                            </div>
                        </div>
                    }

                    @if (ViewBag.mobile.vendor.name == "Flipkart")
                    {
                        <div class="row p-2">
                            <div class="col-xxl-3 col-xl-3 col-lg-4 col-md-4 col-sm-6 col-xs-6 d-flex align-items-center   justify-content-center">
                                <img class="logo   " src="~/User/images/Flipkart-logo.png" alt="Flipkart logo">
                            </div>
                            <div class="col-xxl-3 col-xl-3 d-flex align-items-center hidename  justify-content-center ">
                                <p class="text-black ">  @ViewBag.mobile.name </p>
                            </div>
                            <div class="col-xxl-3 col-xl-3 col-lg-4 col-md-4 col-sm-6 col-xs-6 d-flex align-items-center  justify-content-center ">
                                <p class="text-red fontsize5 fw-bold"> @ViewBag.mobile.price</p>
                            </div>
                            <div class="col-xxl-3 col-xl-3 col-lg-4 col-md-4 col-sm-12 col-xs-12 d-flex align-items-center  justify-content-center block  ">
                                <a class="btn btn-primary fontsize6  col-12  " href=" @ViewBag.mobile.price"> GO TO FLIPKART <i class="fas fa-chevron-right"></i> </a>
                            </div>
                        </div>
                    }
                </div>

