                        </a>
                        <a href="/home/ProductDetails/?vendor=@ViewBag.mobile[1].vendor.name&id=@ViewBag.mobile[1].id" class="pointer">
                            <img class="img-fluid  allproductimg  " id="deta" src="@ViewBag.mobile[1].product_images.image" alt="">
                        </a>
                    }
                    else
                    {
                        <a asp-controller="" asp-action="" class="mt-4"><i class="fas fa-search-plus text-red searchproduct"></i> </a>
                        <form asp-action="ProductsList" class="d-flex flex-column  align-items-center  mx-2 mt-4">
                            <input name="searchStr" type="search" placeholder="Search products" class="form-control radius shadow-none "
                                   aria-label="Search">
                          
                            <button type="submit" class="btn btn-danger redbtn bg-red col-12 m-2 fontsize11 shadow-none">
                                SEARCH
                            </button>

                        </form>

                    }
                </div>
            </div>

            <div class=" col-lg-3 col-md-3  comparehide">
                <div class="card shadow-sm compare d-flex align-items-center justify-content-center  ">
                    @if (ViewBag.n > 2)
                    {
                        <a href="/home/Remove/?id=2&View=CompareChart&list=Comparelist" class=" position-absolute top-0 end-0 text-black pointer pe-3 pt-3 ">
